{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A product category",
    "properties": {
        "description": {
            "description": "The description",
            "type": "string",
            "xDisplay": "Description"
        },
        "name": {
            "description": "The full name of the product",
            "type": "string",
            "xDisplay": "Name"
        },
        "uid": {
            "description": "A unique ID for this item",
            "type": "string",
            "xDisplay": "UID",
            "xRestrictions": "read-only"
        }
    },
    "required": [
        "name",
        "uid"
    ],
    "title": "category",
    "type": "object",
    "xUidPrefix": "category"
}
